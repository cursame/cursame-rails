    <script>
      setInterval(function(){
          if(navigator.onLine){
              $('#scarlate').hide();
              $('body').css('overflow','auto');
          }else{
             $('#scarlate').show(); 
             $('#scarlate').html(' <center><p style="padding-top:5px;"> Sin connexión a la red <b> :( </b>. </p></center>');
             $('body').css('overflow','hidden'); 
          }
          
      },5000);
      
       window.clearInterval(GetIntevalExpire);
       var IntervalFunctionForExpireSession;
       IntervalFunctionForExpireSession = 1800000;
       var GetIntevalExpire = setInterval(function(){ 
           $.get('<%= expire_session_path %>');
       },IntervalFunctionForExpireSession);
       
       //console.log(IntervalFunctionForExpireSession);

      </script>

