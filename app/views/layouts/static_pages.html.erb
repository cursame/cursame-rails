<!DOCTYPE html>
<html class="website-pages">
  <head>
    <%= display_meta_tags site: 'Cúrsame', separator: '·'.html_safe, reverse: true, author: 'Cúrsame', twitter: { card: 'sumary', site: '@cursame' } %>
    <meta name="google-site-verification" content="2iDsswfLsUTCsZEFQ5vlpW4OUwsVwWsqwF5WHqAHuUM" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <%= favicon_link_tag 'favicon.ico' %>
    <%= stylesheet_link_tag 'application', :media => 'all', :debug => true %>
    <%= javascript_include_tag 'application.js' %>
    <%= javascript_include_tag('website/website') %>
    <%= yield :javascripts %>
    <!--[if IE]>
      <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <%= csrf_meta_tags %>
  </head>
  <body class="<%= yield :body_classes %>">
    <% cache_expire %>
    <div class="pages-wrap">
      <%= render partial: '/shared/static_pages/header' %>
      <%= yield %>
      <div class="website-footer">
        <div class="arrow"></div>
        <div class="wrapper">
          <div class="cols-12">
            <div class="col col-3">
              <div class="col-inner">
                <h4><%= t('.first_column.title1') %></h4>
                <ul class="footer-links">
                  <li><%= link_to t('.first_column.title2'), features_path %></li>
                  <li><%= link_to t('.first_column.title3'), success_stories_path %></li>
                  <li><%= link_to t('.first_column.title4'), about_us_path %></li>
                  <li><%= link_to t('.first_column.title5'), press_path %></li>
                  <li><%= link_to t('.first_column.title6'), jobs_path %></li>
                  <li><%= link_to t('.first_column.title7'), contact_path %></li>
                  <li><%= link_to t('.first_column.title8'), request_demo_path %></li>
                  <li><%= link_to t('.first_column.title9'), apps_path %></li>
                  <li><%= link_to t('.first_column.title10'), "http://cursame.tumblr.com/", target: "_blank" %></li>
                </ul>
              </div>
            </div>
            <div class="col col-3">
              <div class="col-inner">
                <h4><%= t('.second_column.title1') %></h4>
                <ul class="footer-links">
                  <li><%= link_to t('.second_column.title2'), "https://cursame.zendesk.com/hc/es", target: "_blank" %></li>
                  <li><%= link_to t('.second_column.title3'), privacidad_path %></li>
                  <li><%= link_to t('.second_column.title4'), conditions_path %></li>
                </ul>
              </div>
            </div>
            <div class="col col-3">
              <div class="col-inner">
                <h4><%= t('.third_column.title1') %></h4>
                <ul class="footer-links">
                  <li>
                    <a href="https://www.facebook.com/Cursameoficial" title="Facebook" target="_blank">
                      <%= icon('follow-fb-white') %> Facebook
                    </a>
                  </li>
                  <li>
                    <a href="https://twitter.com/cursame" title="Twitter"  target="_blank">
                      <%= icon('follow-twitter-white') %> Twitter
                    </a>
                  </li>
                </ul>
              </div>
            </div>
            <div class="col col-3 footer-metadata">
              <%= link_to "/" do %>
                <%= icon('cursame-logo-gray') %>
              <% end %>
              <br>
              <span><%= t('.fourth_column.title1') %></span>
              <div class="lang-changer">

                <div class="dropdown-wrap">
                  <div class="dropdown-pull">
                    <h5>
                      <% if I18n.locale == :es %>
                        <%= icon 'mx-flag-mini' %> Español
                      <% else %>
                        <%= icon 'usa-fag-mini' %> English
                      <% end %>
                      <%= icon 'drop-arrow-white' %>
                    </h5>
                  </div>
                  <div class="dropdown-v right">
                    <i class="tip"></i>
                    <ul>
                      <li>
                        <%= link_to "English", params.merge(:locale => :en) %>
                      </li>
                      <li>
                        <%= link_to "Español", params.merge(:locale => :es) %>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
              
            </div>
          </div>
        </div>
      </div>
    </div>

