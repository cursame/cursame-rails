<div class="user_dashboard_background">
<center>
<h1><b><%=t('.select')%></b></h1></br>
<div class="cursame_network_select">
  <p><div class="first_part">Cursa</div><div class="secont_part">me</div></p>
</div>

</center>
<div class="user_networks_show">
<% if current_role == "superadmin"%>
<%= link_to (t('.goto') + " >>", superadmnin_activities_path)%></br>
<% @network = Network.all%>
<% @network.each do |network|%>
 <center>
<div class="strip_network">
  <%= link_to "#{network.name}", "http://#{network.subdomain}.#{links}"%></br>
</div>
</center>  
<%end%>
<%else%>
<% current_user.networks.each do |network| %>
<center>
<div class="strip_network">
  <%= link_to "#{network.name}", "http://#{network.subdomain}.#{links}"%>
</div>

</center>

<%end%>
<%end%>
</div>
<div class="note_network">
    <p> <i><%=t('.text1')%> </i> </p>

</div>
<div class="avandonar_session">
<%= link_to t('.log_out'), "http://#{links}/users/sign_out" %>
</div>
</div>
