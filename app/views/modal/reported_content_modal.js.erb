<% unless @content.nil? %>
  createModal('<%= escape_javascript(render(partial: "reported_contents/shared/reported_content_modal", locals: { content: @content, type: @type }))%>');
<% else %>
  Notice('error', I18n.t("modal.reported_content_modal.publ_not_found", {locale: "<%= I18n.locale %>"}));
<% end %>
