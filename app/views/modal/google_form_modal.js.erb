<% unless @form.nil? %>
  createModal( '<%= escape_javascript(render(partial: "/google_forms/shared/google_form_modal", locals: { google_form: @form })) %>', 'overlay-informative first-time-survey-overlay');
<% else %>
  Notice('error', I18n.t("modal.google_form_modal.form_not_found", {locale: "<%= I18n.locale %>"}));
<% end %>
