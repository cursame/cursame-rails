      <div id="content_killer<%=typeo%>" class="content_killer"></div>
      <div id="choser<%=typeo%>" class="choser"></div>    
	  <%end%>
  </div>
  
</div>

<script>
   $('#close_youtube_delivery<%=typeo%>').click(function() {
        $('#youtube_content_delivery<%=typeo%>').toggle('blind', 400);
    });
    $('.cancel_link').click ( function() {
		$('.youtube_content_delivery').hide();
    });
</script>

