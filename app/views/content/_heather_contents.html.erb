    <div class="tabCalss">Contenido de recomendado para el tema
      <div class="tabCalssClose" id="close_youtube_delivery<%=typeo%>"></div>
    </div>

		<div id="content_free_<%=typeo%>" class="content_free<%=typeo%> content_free"></div>
		<div id="content_wikipedia<%=typeo%>" style="position: relative;" class="content_wikipedia"></div>
         
  </div>

  <div id="box_for_loads<%=typeo%>" style="width: 100%; ">
