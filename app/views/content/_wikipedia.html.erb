
<%  @page.first(5).each do |acces_wiky| %>
    <% acces_wiky.each do |first| %>
        <% unless first['pages'].nil? %>
            <% first['pages'].each do |key,value| %>
                <p><% @complex_id= value['pageid'] %></p>
                    <% url =  ("http://es.wikipedia.org/wiki/#{value['title']}").to_s%>
                    </br>
                    <div style=" width:550px; height:280px; position:absolute;left:0px; top:0px;" > 
                    </div>
                    
                    <center>   <iframe  scrolling="no"src="<%=url%>#content" style="width:500px;height:240px; padding:10px;  overflow-x: hidden;"></iframe></br></br></center>
                    
              <%= link_to "#{value['title']}", "http://es.wikipedia.org/wiki/#{value['title']}", target: "_blank"%> 
                     
               <div class="button_add" id="add_nested_delivery_content<%= @complex_id %>">
                   Agregar
               </div>
                 
                  <SCRIPT LANGUAGE="JavaScript">
                           
                            
                           $('#add_nested_delivery_content<%= @complex_id%>').click(function() {

                                $('#content_killer').append('<input id="delivery_contents_attributes_<%=@complex_id%>_content" name="delivery[contents_attributes][<%=@complex_id%>][content]" size="30" type="text" value="<%=url%>">');


                            });




                     </SCRIPT>
            <% end %>
        <% end %>
    <%end%>

<%end%>
  