
<% @files.reverse.each do |cf|%>
          <% @name =  "#{cf.file}" %>
          <% @sintetic_name = @name.split('/').last%>
          <% @termination = @name.split('.').last %>
         
  $("#paginate-files").append('<%= escape_javascript( render( :partial => "course_files/kshow", :locals => {:cf => cf, :name => @name, :sintetic_name => @sintetic_name, :termination => @termination, :course => @course }))%>');
  $("#paginador_container").html('<%= link_to "Cargar archivos anteriores", library_in_course_pagination_path(:page => @next), :id => "paginador", :remote => true%>');
<%end%>