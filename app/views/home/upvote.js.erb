$('#liked-<%= @publication.id%>').html('');
$('#liked-<%= @publication.id%>').prepend("<%= escape_javascript(link_to 'Me Gusta', downvote_path(@publication.id), :remote => true) %>");

<% if @publication.publication_type != 'Comment' %>
  $('#liked-<%= @publication.id%> a').html('').html('<i class="i i-like-c-white-active"> </i>')
<% else %>
  $('#liked-<%= @publication.id%> a').html('').html('<i class="i i-like-c-active"> </i>')
<% end %>
$('#counter-like-<%= @publication.id%>').html('');
$('#counter-like-<%= @publication.id%>').html("<%= @publication.publication.votes.up.size %>");
