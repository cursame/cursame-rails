$("#delivery_state_publish<%=@delivery.id%>").html('<%= link_to "#{@delivery.state}", publish_unpublish_delivery_manualy_path(:state => @delivery.state, :id => @delivery.id ), :remote => true, :class => "state-btn" %>');
$("#dates_delivery<%=@delivery.id%>").html('<%= @delivery.publish_date %> <%= @delivery.end_date %>')
$("#notice").html('');
$("#notice").css('color','#608847');
$("#notice").css('background','#DFF0D8');         
$( "#notice" ).animate( { width: "900px", height: "10px", padding: "20px", border: "1px solid #608847"}, { queue: false, duration: 100 });
$('#notice').append('<%= @message %>');
setTimeout(function() {
    $('#notice').html('')
    $( "#notice" ).animate( { width: "0px", padding: "0px", border: "1px solid #608847" }, { queue: false, duration: 100 });
}, 3500);
