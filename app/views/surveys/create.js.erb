<% if !@error %>
	resetForm($('#new_survey'));
	$('#timeline').prepend("<%= escape_javascript(render(:partial => '/shared/publications', :locals => {:wall => [@publication],:visible_for => 'network'}))%>");
	$('#timeline').effect("highlight", {}, 1000);
	$("#notice").html('');
	$("#notice").css('color','#608847');
	$("#notice").css('background','#DFF0D8');
	$( "#notice" ).animate( { width: "900px", height: "10px", padding: "20px", border: "1px solid #608847"}, { queue: false, duration: 100 });
	$('#notice').append('Se ha creado un cuestionario');
	setTimeout(function() {
	    $('#notice').html('')
	    $( "#notice" ).animate( { width: "0px", padding: "0px", border: "1px solid #608847" }, { queue: false, duration: 100 });
	      CleanerBlur();
	      DisableBlured();
	    $('#pk_menu').html('<%= escape_javascript(render(:partial => 'shared/post_menu')) %>');
	    
	}, 1500);
<%else%>
  $("#notice").html('');
  $("#notice").css('color','#FFF');
  $("#notice").css('background','#E63A3A');         
  $( "#notice" ).animate( { width: "900px", height: "10px", padding: "20px", border: "1px solid #608847"}, { queue: false, duration: 100 });
  $('#notice').append('El cuestionario no tiene cursos asignados');
<%end%>