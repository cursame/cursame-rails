<% if !@error %>

  $('#timeline').prepend('<%= escape_javascript(render( :partial => "/shared/posts/posts_list", :locals => {:wall => [@publication], :visible_for => "network"} ))%>');
  Notice('success', I18n.t("surveys.correct_create", {locale: "<%= I18n.locale %>" }));
  newPublicationCleaner();

<% else %>
  Notice('error', I18n.t("surveys.select_course", {locale: "<%= I18n.locale %>"}));
<% end %>
