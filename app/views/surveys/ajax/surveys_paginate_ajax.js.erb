var content = '<%= escape_javascript( render( partial: "surveys/shared/surveys_list_cards", locals: { surveys: @surveys } ) )%>';
$('div.academic-activities-main .cols-12').append( content );

console.log('<%= @surveys.current_page %>', '<%= @surveys.num_pages %>');
<% if @surveys.current_page < @surveys.num_pages %>
	var loadMoreContent = '<%= escape_javascript( render( partial: "surveys/shared/surveys_paginate_button", locals: { page: @next_page, type: @type, course: @course }) )%>';
	$('div.load-more').replaceWith( loadMoreContent );
<% else %>
	var message = '<%= escape_javascript( render( partial: "shared/no_more_records_found", locals: { message: t('.no_more_quest') } ) )%>';
	$('div.load-more div.col-inner').html( message )
<% end %>
