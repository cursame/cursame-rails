<h4>
  {{#if title}}
    {{title}}
  {{/if}}

  {{#if name}}
    {{name}}
  {{/if}}
</h4>

<ul class="list-buttons left">
  <li>
    <span class="meta-info">Fecha de Publicaci√≥n:</span><br>
    {{publish_date}}
  </li>
  <li>
    <span class="meta-info">Fecha de Entrega:</span><br>
    {{end_date}} 
  </li>
  <li>
    <span class="meta-info">Accion:</span><br>
    <div id="delivery_state_publish{{id}}">
    {{#is state "published" }}
      <a href="/publish_unpublish_delivery_manualy?state='unpublish'&id={{id}}" data-remote="true">
        Despublicar
      </a>
    {{else}}
      <a href="/publish_unpublish_delivery_manualy?state='published'&id={{id}}" data-remote="true">
        Publicar
      </a>
    {{/is}}
    </div>
  </li>
</ul>